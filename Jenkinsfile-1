
pipeline {
  agent {label "master"} 
  stages {
    stage('build') {
      steps {
        sh """
          docker.build("test_liquibase_v", "--build-arg jenkinsParam=$jenkinsParam .")
        """
      }
    }
    stage("run") {
      steps {
        sh """
          docker run --rm --network="host" test_liquibase_v
        """
      }
    }
  }
}