
pipeline {
  agent {label "linux"} 
  stages {
    stage('build') {
      steps {
        sh """
          docker build --tag liquibase_v1 .
        """
      }
    }
    stage("run") {
      steps {
        sh """
          docker run --rm liquibase_v1 liquibase test
        """
      }
    }
  }
}