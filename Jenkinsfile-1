
pipeline {
  agent {label "master"} 
  stages {
    stage('build') {
      steps {
        sh """
          docker build -t test_liquibase_v .
        """
      }
    }
    stage("run") {
      steps {
        sh """
          docker run --network="host" test_liquibase_v liquibase updateSQL
        """
      }
    }
  }
}