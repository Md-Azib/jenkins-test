
pipeline {
  agent {label "master"} 
  stages {
    stage('build') {
      steps {
        sh """
          docker build -t test_liquibase_v --build-arg TEAM_NAME="${teamName}" --build-arg DB_NAME="${dbName}" .
        """
      }
    }
    stage("run") {
      steps {
        sh """
          docker run --rm --network="host" test_liquibase_v
        """
      }
    }
  }
}