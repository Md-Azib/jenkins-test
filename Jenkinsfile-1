
pipeline {
  agent {label "linux"} 
  stages {
    stage('build') {
      steps {
        sh """
          docker build -t test_liquibase_v .
        """
      }
    }
    stage("run") {
      steps {
        sh """
          docker run -d --network="host" test_liquibase_v liquibase updateTestingRollback
        """
      }
    }
  }
}