
pipeline {
  agent {label "linux"} 
  stages {
    stage('build') {
      steps {
        sh """
          docker build --tag liquibase_v20 .
        """
      }
    }
    stage("run") {
      steps {
        sh """
          docker run --rm -d --network="host" liquibase_v20 liquibase updateTestingRollback
        """
      }
    }
  }
}