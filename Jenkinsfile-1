
pipeline {
  agent {label "master"} 
  stages {
    stage('build') {
      steps {
        script{
          def repo=docker.build("test_liquibase_v","--build-arg jenkinsParam=$jenkinsParam --no-cache .")
        }
      }
    }
    stage("run") {
      steps {
        sh """
          docker run --rm --network="host" test_liquibase_v
        """
      }
    }
  }
}