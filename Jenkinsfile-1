
pipeline {
  agent {label "master"} 
  stages {
    stage('build') {
      steps {
        sh """
          docker build -t test_liquibase_v --build-arg JENKINS_PARAM="${jenkinsParam}" .
        """
      }
    }
    stage("run") {
      steps {
        sh """
          docker run --rm --network="host" test_liquibase_v
        """
      }
    }
  }
}