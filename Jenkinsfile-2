pipeline {
  agent {
    docker { 
      image 'liquibase/liquibase:4.4.2'
      image 'python:3.5.1' 
    }
  }
  stages {
    stage('Status') {
      steps {
        sh 'chmod +x properties.env'
        sh './properties.env'
        sh 'python3 prog.py'
      }
    }
  }
  post {
    always {
      cleanWs()
    }
  }
}
