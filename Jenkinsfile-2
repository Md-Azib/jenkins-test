pipeline {
  agent {
    docker { 
      image 'liquibase/liquibase:4.4.2'
    }
  }
  stages {
    stage('Status') {
      steps {
        sh 'chmod +x properties.env'
        sh '. ./properties.env'
        sh '/usr/bin/python3 prog.py'
      }
    }
  }
  post {
    always {
      cleanWs()
    }
  }
}
